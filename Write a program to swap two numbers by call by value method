#include<stdio.h>
#include<conio.h>
void main()
{
int a,b;
void swap(int,int);
clrscr();
printf("\n enter two values");
scanf("%d%d",&a,&b);
printf(" before swapping");
printf("a=%d\tb=%d",a,b);
swap(a,b);
printf("\n after swapping");
printf("a=%d\tb=%d",a,b);
getch();
}
void swap(int a,int b)
{
int t;
t=a;
a=b;
b=t;
}
