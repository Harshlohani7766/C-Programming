#include<stdio.h>
#include<conio.h>
void main()
{
int a[3][3],b[3][3],i,j;
clrscr();
for(i=0;i<=2;i++)
{
printf("\n");
for(j=0;j<=2;j++)
{
printf("enter the value");
scanf("\t%d",&a[i][j]);
}
}
for(i=0;i<=2;i++)
{
for(j=0;j<=2;j++)
{
b[i][j]=a[i][j];
}}
for(i=0;i<=2;i++)
{
printf("\n");
for(j=0;j<=2;j++)
printf("\t%d",b[i][j]);
getch();
}}
